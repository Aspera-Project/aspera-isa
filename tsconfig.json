{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "declaration": true,
    "outDir": "./lib",
    "strict": true,
    "lib": [
	"dom",
	"es2015"
    ]
  },
  "include": [
	  "src/**/*"
  ],
  "exclude": [
	"node_modules"
  ],
  "typedocOptions": {
	"mode": "modules",
	"out": "docs"
  }
}
